{
  "name": "express-api-demo",
  "version": "1.0.0",
  "description": "Express.js API Demo with MySQL, Redis and Docker",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "node --watch src/server.js",
    "build": "echo 'No build needed for this project'",
