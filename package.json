    "dotenv": "^16.0.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^8.17.0",
    "pino-http": "^8.6.0",
    "redis": "^4.6.12",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
